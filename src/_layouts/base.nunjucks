<body class="{{bodyClass}}">
  <!-- Google Tag Manager (noscript) -->
  <noscript>
    <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-NMLBVXX" height="0" width="0" style="display:none;visibility:hidden"></iframe>
  </noscript>
  <!-- End Google Tag Manager (noscript) -->
  <!--[if lt IE 9]>
    <p class="browsehappy">
      You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.
    </p>
    <![endif]-->
  <div class="main-content">
    <header class="header-site text-center">
      <h1 class="logo animated bounceIn"><img src="{{baseUrl}}images/logo-hv.svg" alt="Hello victor" class="img-responsive center-block" /></h1>
      {% block header %}{% endblock %}
    </header>
    {% block content %}{% endblock %}
  </div>
  <footer class="footer-site clearfix">
    <div class="footer-block pull-left">
      <div class="footer-block-inner">
        <div>&copy;Hello Victor 2017</div>
        <ul class="menu-footer">
          <li>
            <a href="#">Termes & Services</a>
          </li>
          <li>
            <a href="#">Mentions légales</a>
          </li>
          <li>
            <a href="#">CGV</a>
          </li>
          <li>
            <a href="#">Contactez-nous</a>
          </li>
        </ul>
      </div>
    </div>
    <div class="footer-block  pull-right">
      <div class="footer-block-inner">
        <div class="block-share">
          <a href="http://facebook.com/hellovictorassurances" target="_blank"><i class="icon icon-facebook"></i></a>
          <a href="http://twitter.com/_myvictor" target="_blank"><i class="icon icon-twitter"></i></a>
        </div>
      </div>
    </div>
    <div class="footer-block logo text-center">
      <div class="footer-block-inner">
        <img src="{{baseUrl}}images/logo-footer.svg" alt="Hello Victor - Votre ange gardien" class="img-responsive" />
      </div>
    </div>
    <div class="legal-text">
      Liberty finances capital 1 000 000 €
    </div>
  </footer>
  <script src="{{baseUrl}}scripts/main.js"></script> 
  {% block scripts %}{% endblock %}
  <script src="https://cdn.polyfill.io/v2/polyfill.min.js?features=IntersectionObserver"></script>
</body>

</html>
