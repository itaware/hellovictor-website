    <script src="{{baseUrl}}scripts/jquery-1.12.4.min.js"></script>
    <script src="{{baseUrl}}scripts/jquery.scrollme.min.js"></script>
    <script src="{{baseUrl}}scripts/counter/jquery.counter.js"></script>
    <script src="{{baseUrl}}scripts/main.js"></script>
