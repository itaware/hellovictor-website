    {% block header %}
      <header class="header-site text-center">
        <div class="logo animated bounceIn"><a href="{{baseUrl}}"><img src="{{baseUrl}}images/logo-hv.svg" alt="Hello victor" class="img-responsive center-block" /></a><div>
      </header>
    {% endblock %}
      <div class="main-content">
        {% block content %}{% endblock %}
      </div>
    {% block footer %}
    <footer class="footer-site clearfix">
    <div class="footer-block pull-left">
      <div class="footer-block-inner">
      <div>&copy;Hello Victor 2017</div>
      <ul class="menu-footer">
        <li>
          <a href="#">Termes & Services</a>
        </li>
        <li>
          <a href="#">Mentions légales</a>
        </li>
        <li>
          <a href="#">CGV</a>
        </li>
        <li>
          <a href="#">Contactez-nous</a>
        </li>
      </ul>
    </div>
    </div>
    <div class="footer-block  pull-right">
      <div class="footer-block-inner">
      <div class="block-share">
        <a href="#"><i class="icon icon-facebook"></i></a>
        <a href="#"><i class="icon icon-twitter"></i></a>
      </div>
    </div>
    </div>
    <div class="footer-block logo text-center">
      <div class="footer-block-inner">
      <img src="{{baseUrl}}images/logo-footer.svg" alt="Hello Victor - Votre ange gardien" class="img-responsive" />
    </div>
    </div>
    <div class="legal-text">
      Liberty finances capital 1 000 000 €
    </div>
</footer>
