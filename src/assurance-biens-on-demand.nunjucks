{% extends '_layouts/base.nunjucks' %}
{% block config %}
{# Setup site's base URL to match the "baseUrl" key within `package.json` #}
{# Otherwise default to relative pathing #}
{% set bodyClass = 'product-page product-objects' %}
{% set page = 'objects-courant' %}
{% set slogan = "L'assurance 100% en ligne qui couvre tous vos biens" %}
{% endblock %}
{% block header %}
<div class="header-site-inner">
<div class="header-site-top">
  <h1 class="logo animated bounceIn"><a href="/"><img src="{{baseUrl}}images/logo-hello-victor-white.svg" alt="Hello victor" class="img-responsive center-block" /></a></h1>
  <div class="slogan">Certains de vos biens sont plus importants que les autres.</div>
</div>
<div class="header-site-bottom">
  <div class="container-object">
      <div class="object-item">
        <img src="{{baseUrl}}images/product-type/camera.svg" alt="Appareil photo" />
        <img src="{{baseUrl}}images/product-type/watch.svg" alt="Montre" />
        <img src="{{baseUrl}}images/product-type/smartphone.svg" alt="Téléphone" />
      </div>
      <div class="object-name">
        <a href="form.html" class="btn btn-default">Assurez vos objets précieux</a>
      </div>
  </div>
</div>
</div>
{% endblock %}
{% block content %}
{% include '_components/insurance-level.nunjucks' %} 
{% include '_components/inventory.nunjucks' %} 
{% include '_components/inventory-app-detail.nunjucks' %} 
{% include '_components/claim-declaration.nunjucks' %} 
{% include '_components/share.nunjucks' %} 
{% endblock %}
